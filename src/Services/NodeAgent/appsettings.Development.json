{
  "CloudOption": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "QueueServerTimeoutSeconds": "30",
    "TableServerTimeoutSeconds": "30",
    "VisibleTimeoutSeconds": 60,
    "NodesTableName": "nodestable",
    "IdsTableName": "idstable",
    "JobsTableName": "jobstable",
    "JobDispatchQueueName": "jobdispatchqueue",
    "NodeDispatchQueuePattern": "nodedispatchqueue-{0}",
    "JobPartitionPattern": "job-{0}",
    "JobEntryKey": "jobentry",
    "JobResultPattern": "nodejobresult-{0}"
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
